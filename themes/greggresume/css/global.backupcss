   width: 50%;
  margin-left: 70px;
}

.node-type-packaging .group-left img{
 width: 100%;
}
.node-type-video .group-left{
  width: 66%;

  margin-right: 20px;
 
}
.node-type-video .group-middle{
  width: 30%;
}

.node-type-video .group-right{
