  width: 100%;
}
.block-views-web-block h2.block-title,
.block-views-motion-block h2.block-title,
.block-views-design-block h2.block-title{
    
}

#superfish-2{
  width: 70%;
  font-size: 14px;
}

body {
  font: 13px/1.5 Verdana, Helvetica, Arial, sans-serif;
  color: #111;
  font-size: 14px;
  background-image: url(../images/lightgrungegrid.jpg);
}

a:link, a:visited {
  color: #009ea3;
  text-decoration: none;
}

a:hover {
  color: #ccc;
  text-decoration: none;
}

a:active {
  color: #00bafa;
  text-decoration: none;

}

a.active-trail,
a.active.active{
  
  color: #00bafa;
  text-decoration: none;
}


hr {
  margin: 0;
  padding: 0;
  border: none;
  height: 1px;
  background: #5294c1;
}

h1, h2, h3, h4, h5, h6 {
  letter-spacing: -0.05em;
  
}

h1 {
  font-size: 36px;
}

h1#page-title{
  
  color: black;
  background-color: rgba(255,254,255,0.6);
  padding: 5px;
line-height: 0.8em;
}

.node-type-design h1#page-title,
.node-type-packaging h1#page-title,
.node-type-business-card h1#page-title,
.node-type-logo h1#page-title,
.node-type-tshirt h1#page-title,
.node-type-mobile h1#page-title,
.node-type-video h1#page-title,
.node-type-theatre h1#page-title,
.node-type-mobile h1#page-title,
.node-type-website h1#page-title{
  display: none;
}

h2 span.amp{

}

h3 span.amp{
  font-size: 200%;
  position: relative;
  top:5px;
  padding-left: 5px;
  padding-right: 5px;
}

.view-mode-avatar{
  width: 80px;
  float:left;
  margin-right: 20px;
}

h2 {
  font-size: 28px;
  margin-bottom: 10px !important;
}

.group-middle h2{
  line-height: .9em;
}

/* Make site titles the same size if it's an H1 (home) or H2 (internal) */
h1.site-name,
h2.site-name a {
  font-size: 86px;
  padding: 3px;
}



/* Make it a bit smaller for block headers */
h2.block-title {
  font-size: 18px;
  margin: 0;	
}

h2.node-title {
  font-size: 28px;
}



h3.nodetype{
  color: black;
  position: absolute;
  right: 20px;
  top: -40px;
  font-size: 35px;
z-index: 999;
  text-transform: capitalize;
  background-color: white;
height: 40px;
}

h4 {
  font-size: 19px;
}


h5 {
  font-size: 17px;
}

h6 {
  font-size: 22px;
}

ol {
  list-style: decimal;
}

ul {
  list-style: disc;
  margin-left: 50px;
}

.tabs.primary a{
  background-color: black;
  padding: 5px;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border: none;
}
ul.primary{
  border-bottom: none;
}

ul ul,
ol ol {
  padding-top: 0.5em;
}

.experience-row{
  padding-bottom: 15px;
  position: relative;
  padding: 20px;
  background-color: #efefef;
  margin-bottom: 10px;
  

}
.experience-row p{
  margin-bottom: 10px;

  font-size: 14px !important;
}

.experience-row h4{
  font-size: 18px;
  color:#888;
  position: relative;
  float: right;
   margin-bottom: 5px;

}

.experience-row h4.web-url{
   font-size: 14px;
   position: absolute;
  margin-bottom: 5px;
  top: 60px;
  right: 20px;
}

.experience-row h3.position{
  font-size: 18px;
  color:#444;
   width: 80%;
   margin-bottom: 5px;
   clear: both;
   width:100%;
border-top: thin #ccc solid;
 
}
.experience-row h3.company{
  margin-bottom: 5px;
  float: left;
  margin-right: 10px;
  font-size: 22px;
  
}



.twocolumn{
  -moz-column-count: 2;
-moz-column-gap: 1em;
-webkit-column-count: 2;
-webkit-column-gap: 1em;

}

.view-mode-full img{
 border: 8px solid white;
  -webkit-box-shadow: 0px 2px 4px #888;
  -moz-box-shadow: 0px 2p 4px #888;
  box-shadow: 0px 2p 4px #888;
 margin-bottom: 10px;
}

.flexslider img{
  border: none;
  
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}

.group-header{
  padding-bottom: 3px;
  border-bottom: thin white solid;
  margin-bottom: 30px;
  
}

.group-header span{
 margin-left: 10px;
 margin-right: 5px;
 padding-left: 10px;
  border-left: thin white solid ;

}

