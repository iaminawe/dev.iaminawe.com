/* This file will hold the main styles for your website. */

.block{

  padding: 10px;
}



a{
  color: #ccc;
  text-decoration: none;
}

.logo-img{
  width: 70%;
  left: 120px;
  position: relative;
  top: 20px;
}

.logo-img img{
  max-width: 100%;
  margin: none !important;
}

.flexslider{
   border: 8px solid white;
  -webkit-box-shadow: 0px 3px 4px #888;
  -moz-box-shadow: 0px 3px 4px #888;
  box-shadow: 0px 3px 4px #888;
  width: 98%;
  padding: 5px;
}

#block-views-web-block,
#block-views-motion-block,
#block-views-design-block{
    width: 30%;
    float: left;
}  

#flexslider_views_slideshow_main_web-block,
#flexslider_views_slideshow_main_motion-block,
